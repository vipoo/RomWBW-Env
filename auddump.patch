diff --git a/Source/HBIOS/audio.asm b/Source/HBIOS/audio.asm
index 68f4110..28eabf9 100644
--- a/Source/HBIOS/audio.asm
+++ b/Source/HBIOS/audio.asm
@@ -60,6 +60,10 @@ AUD_DISPATCH:
 	JP	Z, AUD_QUERY
 	CP	BF_AUDDUR
 	JP	Z, AUD_DURATION
+
+	CP	BF_AUDDMP
+	JP	Z, AUD_DUMP
+
 	POP	IY
 	OR	$FF
 	RET
@@ -187,6 +191,60 @@ AUD_DURATION:
 	XOR	A
 	RET
 
+AUD_DUMP:
+	LD	HL, QUEUE_CMDBUF
+	LD	BC, QUEUED_CMD_SIZE * QUEUED_CMD_CNT / 4
+	LD	DE, NEWLINE
+
+AUD_DUMP1:
+	AUDTRACE_HL
+
+	LD	DE, STR_SPACE
+	CALL	WRITESTR
+
+	LD	DE, STR_SPACE
+	CALL	WRITESTR
+
+	LD	A, (HL)
+	CALL	PRTHEXBYTE
+	INC	HL
+
+	LD	DE, STR_SPACE
+	CALL	WRITESTR
+
+	LD	A, (HL)
+	CALL	PRTHEXBYTE
+	INC	HL
+
+	LD	DE, STR_SPACE
+	CALL	WRITESTR
+
+	LD	A, (HL)
+	CALL	PRTHEXBYTE
+	INC	HL
+
+	LD	DE, STR_SPACE
+	CALL	WRITESTR
+
+	LD	A, (HL)
+	CALL	PRTHEXBYTE
+	INC	HL
+
+	CALL	NEWLINE
+
+	DEC	BC
+	LD	A, B
+	OR	C
+	JR	NZ, AUD_DUMP1
+
+	LD	DE, STR_BUFFER_DATA
+	CALL	WRITESTR
+
+	POP	IY
+
+	XOR	A
+	RET
+
 ; -------------------------------
 
 AUD_STARTDUR:
