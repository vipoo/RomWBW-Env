
;Boot sound on

./expect-lines 1 sound-ay-boot-beep-on.fixture
./expect-lines 0 sound-ay-boot-beep-off.fixture
./clear

; play not yet
hbios 54 00 01 00
./expect 1 "Ret AF: 0xFF"
./clear

; inc time 20 (1/3 of a second)
hbios F9 D2 14 00
./expect 1 "Ret AF: 0x00"
./expect-lines 1 sound-ay-boot-beep-off.fixture
./clear

; can play now
hbios 54 00 01 00
./expect 1 "Ret AF: 0x00"
./clear

; inc time
hbios F9 D2 02 00
./expect 1 "Ret AF: 0x00"
./clear

; can still play now
hbios 54 00 01 00
./expect 1 "Ret AF: 0x00"
