diff --git a/Source/HBIOS/hbios.asm b/Source/HBIOS/hbios.asm
index ad19494..f2bb9ac 100644
--- a/Source/HBIOS/hbios.asm
+++ b/Source/HBIOS/hbios.asm
@@ -362,6 +362,30 @@ HBX_ROM:
 	RET				; DONE
 #ENDIF
 #IF (MEMMGR == MM_Z2)
+
+#IFDEF DINOTRONHACK
+	BIT	7,A			; BIT 7 SET REQUESTS RAM PAGE
+	JR	Z, HBX_ROMS		; NOT SET, MUST BE ONE OF THE ROMS
+HBX_RAM:
+	RES	7,A			; RAM PAGE REQUESTED: CLEAR ROM BIT
+	ADD	A,16			; ADD 16 X 32K - RAM STARTS FROM 512K
+	RLCA				; TIMES 2 - GET 16K PAGE INSTEAD OF 32K
+	SET	6, A			; FLIP BITS DUE TO DINO MISWIRING
+	RES	5, A
+	JR	HBX_GO			; A CONTAINS RAM BANK SELECTION VALUE
+;
+HBX_ROMS:
+	BIT	4,A			; BIT 4 INDICATES EXTERNAL ROM
+	JR	Z, HBX_ROM1		; NO SET, MUST BE THE INTERNAL ROM
+
+	RLCA				; TIMES 2 - GET 16K PAGE INSTEAD OF 32K
+	JR	HBX_GO			;
+
+HBX_ROM1:
+	RLCA				; TIMES 2 - GET 16K PAGE INSTEAD OF 32K
+
+HBX_GO:
+#ELSE
 	BIT	7,A			; BIT 7 SET REQUESTS RAM PAGE
 	JR	Z,HBX_ROM		; NOT SET, SELECT ROM PAGE
 	RES	7,A			; RAM PAGE REQUESTED: CLEAR ROM BIT
@@ -369,6 +393,7 @@ HBX_ROM:
 ;
 HBX_ROM:
 	RLCA				; TIMES 2 - GET 16K PAGE INSTEAD OF 32K
+#ENDIF
 	OUT	(MPGSEL_0),A		; BANK_0: 0K - 16K
 	INC	A			;
 	OUT	(MPGSEL_1),A		; BANK_1: 16K - 32K
@@ -917,7 +942,11 @@ HB_START:
 	INC	A
 	OUT	(MPGSEL_1),A
 #ENDIF
+#IFDEF DINOTRONHACK
+	LD	A,$5E
+#ELSE
 	LD	A,62
+#ENDIF
 	OUT	(MPGSEL_2),A
 	INC	A
 	OUT	(MPGSEL_3),A
@@ -4803,7 +4832,11 @@ HB_BATCOND	.DB	0		; BATTERY CONDITION (0=LOW, 1=OK)
 HB_BOOT_REC	.DB	0		; BOOT MODE (0=NORMAL, 1=RECOVERY MODE)
 #ENDIF
 ;
+#IFDEF DINOTRONHACK
+STR_BANNER	.DB	"DINOTRON v", BIOSVER, ", ", TIMESTAMP, "$"
+#ELSE
 STR_BANNER	.DB	"RomWBW HBIOS v", BIOSVER, ", ", TIMESTAMP, "$"
+#ENDIF
 STR_PLATFORM	.DB	PLATFORM_NAME, "$"
 STR_SWITCH	.DB	"*** Activating CRT Console ***$"
 STR_BADINT	.DB	"\r\n*** BAD INT ***\r\n$"
